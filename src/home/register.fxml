<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="512.0" prefWidth="669.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="home.RegisterController">
    <children>
        <AnchorPane layoutY="-2.0" prefHeight="101.0" prefWidth="669.0" style="-fx-background-color: #006400;">
            <children>
                <ImageView fx:id="img1View" fitHeight="49.0" fitWidth="78.0" layoutX="32.0" layoutY="20.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../images/picture/adb9b26aa4e445578989d19e07898ff4.png" />
                    </image>
                </ImageView>
                <Label layoutX="104.0" layoutY="35.0" text="User Register" textFill="#fffafa">
                    <font>
                        <Font size="24.0" />
                    </font>
                </Label>
            </children>
        </AnchorPane>
        <Label layoutX="52.0" layoutY="144.0" text="用户名" />
        <Label layoutX="54.0" layoutY="201.0" text="密码" />
        <Label layoutX="40.0" layoutY="262.0" text="确认密码" />
        <TextField fx:id="UsernameOfRegister" layoutX="176.0" layoutY="139.0" prefHeight="30.0" prefWidth="400.0" promptText="username" />
        <PasswordField fx:id="setPasswordOfRegister" layoutX="176.0" layoutY="196.0" prefHeight="30.0" prefWidth="400.0" promptText="password" />
        <PasswordField fx:id="confirmPasswordOfRegister" layoutX="176.0" layoutY="257.0" prefHeight="30.0" prefWidth="400.0" promptText="Confirm Password" />
        <Button fx:id="registerButton" layoutX="135.0" layoutY="400.0" mnemonicParsing="false" onAction="#RegisterNewAcount" prefHeight="30.0" prefWidth="400.0" style="-fx-background-color: #006400;" text="注册" textFill="#fcf6f6" />
        <Button fx:id="closeButton" layoutX="355.0" layoutY="459.0" mnemonicParsing="false" onAction="#CloseAndSignOut" prefHeight="30.0" prefWidth="180.0" style="-fx-background-color: #006400;" text="关闭" textFill="#eeebeb" />
      <Label layoutX="49.0" layoutY="328.0" prefHeight="20.0" prefWidth="79.0" text="昵称" />
      <TextField fx:id="NicknameOfRegister" layoutX="176.0" layoutY="323.0" prefHeight="30.0" prefWidth="400.0" promptText="nickname" />
      <Button fx:id="BtnReturnToLogin" layoutX="135.0" layoutY="459.0" mnemonicParsing="false" onAction="#returnToLogin" prefHeight="30.0" prefWidth="180.0" style="-fx-background-color: #006400;" text="返回" textFill="#eeebeb" />
    </children>
</AnchorPane>
